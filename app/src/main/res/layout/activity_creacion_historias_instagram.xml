<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg_color"
    tools:context=".Instagram.CreacionHistoriasInstagram">

    <Button
        android:id="@+id/buttonAprender12"
        android:layout_width="296dp"
        android:layout_height="67dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/boton_redondo_menu"
        android:onClick="EmpezarPantallaCreacionHistoriasInstagram"
        android:text="@string/empezar"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView24" />

    <ImageView
        android:id="@+id/imageView41"
        android:layout_width="321dp"
        android:layout_height="128dp"
        android:layout_marginTop="28dp"
        android:contentDescription="@string/todo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logo"
        tools:ignore="ImageContrastCheck" />

    <TextView
        android:id="@+id/textView24"
        android:layout_width="324dp"
        android:layout_height="346dp"
        android:layout_marginTop="10dp"
        android:gravity="center"
        android:text="@string/hora_de_aprender_lo_que_aprendiste_trata_de_replicar_las_fotos"
        android:textColor="#ffffff"
        android:textSize="37sp"
        android:textStyle="bold|italic"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView41"
        tools:ignore="TextContrastCheck" />
</androidx.constraintlayout.widget.ConstraintLayout>